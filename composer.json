{
    "name": "rseon/mallow",
    "type": "project",
    "description": "Another PHP framework",
    "homepage": "https://github.com/rseon/mallow",
    "require": {
        "php": "^7.1",
        "maximebf/debugbar": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Rseon\\Mallow\\": "src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php src/bin/keygen"
        ]
    }
}
